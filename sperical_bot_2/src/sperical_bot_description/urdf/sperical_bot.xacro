<?xml version="1.0" ?>
<robot name="sperical_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find sperical_bot_description)/urdf/materials.xacro" />
<xacro:include filename="$(find sperical_bot_description)/urdf/sperical_bot.trans" />
<xacro:include filename="$(find sperical_bot_description)/urdf/sperical_bot.gazebo" />
<xacro:include filename="$(find sperical_bot_description)/urdf/gazebo_control.xacro" />
<xacro:include filename="$(find sperical_bot_description)/urdf/tof_camera.xacro" />

<link name="world">
</link>

<link name="base_link">
  <inertial>
    <origin xyz="-0.0023817768660842585 1.1921434354856235e-10 0.05958743189588745" rpy="0 0 0"/>
    <mass value="0.5"/>
    <inertia ixx="0.000107" iyy="0.000206" izz="0.000104" ixy="-0.0" iyz="-0.0" ixz="-4e-06"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>


<link name="left_wheel_1">
  <inertial>
    <origin xyz="-1.0579080899764398e-17 0.02771050311612378 1.4144296366966347e-05" rpy="0 0 0"/>
    <mass value="0.3"/>
    <inertia ixx="0.000207" iyy="0.000339" izz="0.000207" ixy="0.0" iyz="-0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.01 -0.064" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/left_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="gray"/>
  </visual>
  <collision>
    <origin xyz="-0.0 -0.01 -0.064" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/left_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>


<link name="right_wheel_1">
  <inertial>
    <origin xyz="-6.346470725818991e-17 -0.02770577739860361 1.414211746161853e-05" rpy="0 0 0"/>
    <mass value="0.3"/>
    <inertia ixx="0.000207" iyy="0.000339" izz="0.000207" ixy="-0.0" iyz="0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 0.01 -0.064" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/right_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="gray"/>
  </visual>
  <collision>
    <origin xyz="-0.0 0.01 -0.064" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/right_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="caster_wheel_1">
  <inertial>
    <origin xyz="4.926787070605454e-07 9.414691248821328e-16 8.994541222939745e-16" rpy="0 0 0"/>
    <mass value="0.05"/>
    <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="-0.0" iyz="0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.049704 -0.0 -0.0061" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/caster_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="gray"/>
  </visual>
  <collision>
    <origin xyz="0.049704 -0.0 -0.0061" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/caster_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="sensor_link_1">
  <inertial>
    <origin xyz="0.02890454618174197 1.719751742775399e-13 0.03499990805281086" rpy="0 0 0"/>
    <mass value="0.02"/>
    <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" iyz="-0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0205 -0.0 -0.028834" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/sensor_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="red"/>
  </visual>
  <collision>
    <origin xyz="-0.0205 -0.0 -0.028834" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find sperical_bot_description)/meshes/sensor_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="base_link_joint" type="fixed">
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <parent link="world"/>
  <child link="base_link"/>
</joint>

<joint name="Revolute 4" type="continuous">
  <origin xyz="0.0 0.01 0.064" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="left_wheel_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

<joint name="Revolute 5" type="continuous">
  <origin xyz="0.0 -0.01 0.064" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="right_wheel_1"/>
  <axis xyz="0.0 1.0 0.0"/>
</joint>

<joint name="ball" type="continuous">
  <origin xyz="-0.049704 0.0 0.0061" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="caster_wheel_1"/>
</joint>

<joint name="Rigid 7" type="fixed">
  <origin xyz="0.0205 0.0 0.028834" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="sensor_link_1"/>
</joint>


</robot>
